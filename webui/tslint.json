{
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-react-hooks"
  ],
  "rules": {
    "arrow-parens": false,
    "interface-name": false,
    "no-submodule-imports": [false, "@material-ui"],
    "object-literal-sort-keys": false,
    "react-hooks-nesting": "error",
    "trailing-comma": false
  },
  "linterOptions": {
    "exclude": ["**/*.js"]
  }
}
